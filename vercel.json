{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/ics/(.*)",
            "dest": "server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/public/index.html"
        }
    ]
}
